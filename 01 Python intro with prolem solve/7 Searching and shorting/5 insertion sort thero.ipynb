    "def insertionSort(arr):\n",
    "    for i in range(1,len(arr)):\n",
    "        j=i-1\n",
    "        temp=arr[i]\n",
    "        while j>=0 and arr[j]>temp:\n",
    "            arr[j+1]=arr[j]\n",
    "            j=j-1\n",
    "        arr[j+1]=temp\n",
    "    return arr   \n",
    "\n",
    "arr= [9,8,5,6,7,1]\n",
    "a= insertionSort(arr)\n",
    "print(a)"
   ]
  }
