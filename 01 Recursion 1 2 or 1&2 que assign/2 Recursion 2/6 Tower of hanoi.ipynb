    "def towerHanoi(n,a,b,c):\n",
    "    if n==1:\n",
    "        print(\"move the 1st disk\", a,\"to\" ,c)\n",
    "        return\n",
    "    towerHanoi(n-1,a,c,b)\n",
    "    print(\"move\" , n,\"th the disk from\", a, \" to\", c)\n",
    "    towerHanoi(n-1,b,a,c)\n",
    "    \n",
    "# towerHanoi(2,\"s\",\"h\",\"d\")    \n",
    "towerHanoi(4,\"s\",\"h\",\"d\")\n",
    "        "
