    "def printStar(str):\n",
    "    if len(str)==1:\n",
    "        return str\n",
    "    smallstr=printStar(str[1:])\n",
    "    if str[0]==str[1]:\n",
    "        return str[0]+ \"*\" + smallstr\n",
    "    return  str[0] + smallstr\n",
    "    \n",
    "str=input()\n",
    "# str=\"hello\" \n",
    "# str=\"aaaa\"\n",
    "print(printStar(str))    "
   ]
  }
