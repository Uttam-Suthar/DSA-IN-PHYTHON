   "source": [
    "def printSub(s,o):\n",
    "    if len(s)==0:\n",
    "        print(o)\n",
    "        return \n",
    "    # not including 0th char\n",
    "    printSub(s[1:],o)\n",
    "    \n",
    "    # including 0th char\n",
    "    newoutput=o+s[0]\n",
    "    printSub(s[1:],newoutput)"
   ]
